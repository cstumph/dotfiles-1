{
  "title": "tmux-layout-jbmbr",
  "windows": [
    {
      "title": "zsh",
      "layout": {
        "left": "tmux move-window -t 1; z dot; brew update; brew outdated"
        , "right": "z dot; npm-check-updates -g"
      }
    },
    {
      "title": "constructable",
      "layout": {
        "top": {
          "left": "z constructable && nodemon -e js,json,hbs develop.js",
          "right": "z constructable;tmux move-window -t 3;  tmux select-layout -t 3 even-horizontal"
        },
        "bottom": "z constructable; nodemon --ignore node_modules/ -x npm -- test"
      }
    },
    {
      "title": "atomify",
      "layout": {
        "left": "z atomify; tmux move-window -t 4",
        "right": "z atomify"
      }
    },
    {
      "title": "blog",
      "layout": {
        "left": "z staticblog; tmux move-window -t 6",
        "right": "z staticblog"
      }
    },
    {
      "title": "racheandjoey",
      "layout": {
        "top": {
          "left": "z rachelandjoey",
          "right": "z rachelandjoey; tmux move-window -t 5; tmux select-layout -t 5 even-horizontal"
        },
        "bottom": "z rachelandjoey"
      }
    },
  ]
}
