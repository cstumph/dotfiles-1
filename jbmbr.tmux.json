{
  "title": "tmux-layout-jbmbr",
  "windows": [
    {
      "title": "zsh",
      "layout": {
        "left": "cd ~; brew update; brew outdated"
        , "right": "cd ~; npm-check-updates -g"
      }
    },
    {
      "title": "constructable",
      "layout": {
        "top": {
          "left": "z constructable && nodemon -i node_modules develop.js",
          "right": "z constructable; tmux select-layout -t 2 even-horizontal"
        },
        "bottom": "z constructable; nodemon -i node_modules -x npm -- test"
      }
    },
    {
      "title": "tussle",
      "layout": {
        "top": {
          "left": "z tussle && nodemon",
          "right": "z tussle; tmux select-layout -t 3 even-horizontal"
        },
        "bottom": "z tussle"
      }
    },
    {
      "title": "blog",
      "layout": {
        "left": "z staticblog && grunt code",
        "right": "z staticblog"
      }
    },
    {
      "title": "pocket",
      "layout": {
        "left": "z readinglist-pocket",
        "right": "z readinglist-pocket && nodemon -x mocha -i node_modules -- test"
      }
    }
  ]
}
